!------------------!
! REGRESSION MODEL !
!------------------!

!------------------------------------!
! Keywords for the target properties !
!------------------------------------!
ptype=1
ntask=1
nsample=5
task_weighting=1
desc_dim=4
restart=.false.

!------------------------------------------------------------------------------!
! Keywords for feature construction (FC) and sure independence screening (SIS) !
!------------------------------------------------------------------------------!
nsf=4
rung=0
opset='(+)(-)(*)(/)(exp)(exp-)(^-1)(^2)(^3)(sqrt)(cbrt)(log)(|-|)(scd)(^6)'
maxcomplexity=10
maxfval_lb=0.001000000000
maxfval_ub=100000.000000000000
subs_sis=1

!------------------------------------------------------------------!
! Keyword for descriptor identification via a sparsifying operator !
!------------------------------------------------------------------!
method='L0'
fit_intercept=.true.
metric='RMSE'
nm_output=100